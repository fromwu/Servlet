<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>servletdemo</display-name>
  <servlet>
    <servlet-name>SayHello</servlet-name>
    <servlet-class>SayHello</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SayHello</servlet-name>
    <url-pattern>/com.yiibai/SayHello</url-pattern>
  </servlet-mapping>

	<servlet>
	    <servlet-name>HeaderServlet</servlet-name>
	    <servlet-class>HeaderServlet</servlet-class>
	</servlet>
	<servlet-mapping>
	    <servlet-name>HeaderServlet</servlet-name>
	    <url-pattern>/com.yiibai/HeaderServlet</url-pattern>
	</servlet-mapping>
  
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>